{
    "manifest_version": 2,
    "name": "104 Overtime Assistent",
    "version": "0.9",
    "description": "計算104工時的貼心好夥伴",
    "icons": {
      "16": "image/icon.png",
      "48": "image/icon.png",
      "128": "image/icon.png"
    },
    "browser_action": {
      "default_title": "fuck104"
    },
    
    "background":
    {
      "scripts": ["jquery-3.5.0.min.js","backgroud.js"],
      "persistent": false
    },

    "permissions": ["activeTab"],

    "content_scripts" : [
    {
      "js" : ["jquery-3.5.0.min.js","content.js"],
      "css":["content.css"],
      "matches" : [ "https://pro.104.com.tw/psc2" ]
      
    }
    ],
    
    "web_accessible_resources": [
      "image/LOGOB.svg",
      "image/icon.png"
    ]
}